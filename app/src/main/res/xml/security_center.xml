        android:title="@string/security_center_privacy_title"
        android:key="prefs_key_security_center_privacy_safety"
        android:fragment="com.sevtinge.hyperceiler.ui.fragment.app.securitycenter.PrivacySafetySettings" />

    <Preference
        android:title="@string/security_center_newbox_title"
        android:fragment="com.sevtinge.hyperceiler.ui.fragment.app.securitycenter.SidebarSettings" />

    <Preference
        android:title="@string/security_center_other_title"
        android:fragment="com.sevtinge.hyperceiler.ui.fragment.app.securitycenter.OtherSettings" />



</PreferenceScreen>