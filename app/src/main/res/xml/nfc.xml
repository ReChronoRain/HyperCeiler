<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:myLocation="@string/nfc">
    <PreferenceCategory>

        <SwitchPreference
            android:title="@string/nfc_disable_sound"
            android:key="prefs_key_nfc_disable_sound"
            android:defaultValue="false" />

        <SwitchPreference
            android:title="@string/nfc_allow_information_screen"
            android:key="prefs_key_nfc_allow_information_screen"
            android:defaultValue="false" />

    </PreferenceCategory>

</PreferenceScreen>